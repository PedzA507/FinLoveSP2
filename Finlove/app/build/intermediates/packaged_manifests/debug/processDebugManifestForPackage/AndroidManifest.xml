            android:theme="@style/Theme.Finlove.NoActionBar" />
        <activity
            android:name="th.ac.rmutto.finlove.RegisterActivity2"
            android:exported="false"
            android:theme="@style/Theme.Finlove.NoActionBar" />
        <activity
            android:name="th.ac.rmutto.finlove.RegisterActivity1"
            android:exported="true"
            android:theme="@style/Theme.Finlove.NoActionBar" />
        <activity
            android:name="th.ac.rmutto.finlove.RegisterActivity"
            android:exported="true"
            android:theme="@style/Theme.Finlove.NoActionBar" />
        <activity
            android:name="th.ac.rmutto.finlove.ForgotPasswordActivity"
